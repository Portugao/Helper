{# Purpose of this template: Display item information for previewing from other modules #}
<dl id="productClass{{ productClass.getKey() }}">
<dt>{{ productClass|muhelpermodule_formattedTitle|notifyFilters('helper.filter_hooks.productclasses.filter')|safeHtml }}</dt>
{% if productClass.description is not empty %}<dd>{{ productClass.description }}</dd>{% endif %}
</dl>
